<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.volleyballclient">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="Join Volley!"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">

        <activity android:name=".MenuBaseActivity"
            android:label="@string/title_activity_menu_nevigation"/>
        <activity android:name=".PasswordActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".RegisterActivity"/>
        <activity android:name=".HomeActivity"/>
        <activity android:name=".EnrollActivity"/>
        <activity android:name=".EnrollFriendActivity"/>
        <activity android:name=".ResponseActivity"/>
        <activity android:name=".DataActivity"/>
        <activity android:name=".StatsActivity"/>
        <activity android:name=".Training" />
    </application>

</manifest>