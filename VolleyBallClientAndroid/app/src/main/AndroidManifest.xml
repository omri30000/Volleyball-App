<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.volleyballclient">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="Join Volley!"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name="com.Sharp.volleyballclient.MenuNevigation"
            android:label="@string/title_activity_menu_nevigation"></activity>
        <activity android:name=".PasswordActivity">
 <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".HomeActivity"></activity>
        <activity android:name=".EnrollActivity"></activity>
        <activity android:name=".EnrollFriendActivity"></activity>
        <activity android:name=".ResponseActivity"></activity>
        <activity android:name=".DataActivity"></activity>
        <activity android:name=".StatsActivity"></activity>
    </application>

</manifest>